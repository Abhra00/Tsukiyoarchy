if [[ $(tty) == *"pts"* ]]; then
    fastfetch --logo-recache # NOTE:: LOGO re-caching required because the bg.sqre will change every time you change the wallpaper
else
    if [ -f /bin/hyprctl ]; then
        echo "Start Hyprland with command Hyprland"
    fi
fi
